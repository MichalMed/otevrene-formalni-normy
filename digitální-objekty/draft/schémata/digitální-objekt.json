{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "https://ofn.gov.cz/digitální-objekty/draft/schémata/digitální-objekt.json",
	"type": "object",
	"title": "Digitální objekt",
	"required": ["typ"],
	"properties": {
		"typ": {
			"const": "Digitální objekt"
		},
		"iri": {

			"type": "string",
			"format": "iri",
			"title": "Identifikátor",
			"examples": [
				"https://www.trebic.cz/zdroj/aktualita/2020/dopadení-lupiče-na-francouzské-ulici"
			]
		},
		"název": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/text.json"
		},
		"popis": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/text.json"
		},
		"vytvořeno": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-okamžik.json"
		},
		"relevantní_do": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-okamžik.json"
		},
		"zneplatněno": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-okamžik.json"
		},
		"příloha": {
			"$ref": "https://ofn.gov.cz/digitální-objekty/draft/schémata/digitální-objekty.json"
		},
		"url": {
			"type": "string",
			"format": "url",
			"title": "URL",
			"examples": [
				"https://www.brno.cz/fileadmin/user_upload/sprava_mesta/magistrat_mesta_brna/PO/volna_mista/VR_067_20_OUPR_urbanista_-_bydleni_05.docx"
			]
		},
		"autor": {
			"type": "array",
			"items": {
				"$ref": "https://ofn.gov.cz/lidé-a-osoby/draft/schémata/člověk-či-osoba.json"
			}
		},
		"typ_media": {
			"type": "string",
			"format": "iri",
			"pattern": "^http\\:\/\/www\\.iana\\.org\/assignments\/media-types\/.*$",
			"title": "Typ media",
			"examples": [
				"http://www.iana.org/assignments/media-types/image/tiff"
			]
		},
		"podmínky_užití": {
			
		}
	}
}