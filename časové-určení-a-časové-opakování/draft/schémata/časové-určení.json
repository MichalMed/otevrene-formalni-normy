{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://ofn.gov.cz/časové-určení-a-časové-opakování/draft/schémata/časové-určení.json",
    "type": "object",
    "title": "Časové určení",
    "properties": {
        "interval": {
            "$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-interval.json"
