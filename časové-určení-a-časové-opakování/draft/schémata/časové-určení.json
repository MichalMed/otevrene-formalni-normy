        "výjimka": {
            "$ref": "https://ofn.gov.cz/časové-určení-a-časové-opakování/draft/schémata/časové-určení.json"
        },
	    "opakování": {
            "$ref": "https://ofn.gov.cz/časové-určení-a-časové-opakování/draft/schémata/časové-opakování.json"
        },
        "specifické_časové_určení": {
