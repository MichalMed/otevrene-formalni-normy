			<xs:element name="kód_obce" type="xs:string" minOccurs="0"/>
			<xs:element name="kód_okresu" type="xs:string" minOccurs="0"/>
			<xs:element name="kód_ruian" type="xs:integer" minOccurs="0"/>
			<xs:element name="místnost" type="xs:string" minOccurs="0"/>
