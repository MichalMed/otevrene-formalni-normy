			<xs:element name="url" type="xs:string" minOccurs="0"/>
			<xs:element name="znak_čísla_orientačního" type="xs:string" minOccurs="0"/>
			<xs:any namespace="##other" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
