{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://data.gov.cz/otevřené-formální-normy/registr-práv-a-povinností/převod-úkonů-v-rámci-působnosti.schema.json",
  "type": "object",
  "required": [
    "položky"
  ],
  "properties": {
    "položky": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "úkon": {
            "type": "string",
            "examples": [
              "úkon/U3"
            ],
            "pattern": "^úkon/U[0-9]+$"
          },
          "z-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A4997/00022985"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+-[0-9]+$"
          },
          "do-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A4997/00007064"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+-[0-9]+$"
          },
          "přebírající-pracoviště": {
            "type": "array",
            "examples": [
              "pracoviště/19831"
            ],
            "items": {
              "type": "string",
              "pattern": "^pracoviště/[0-9]+$"
            }
          },
          "od": {
            "type": "string",
            "examples": [
              "2018-09-08"
            ],
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "do": {
            "type": "string",
            "examples": [
              "2018-11-03"
            ],
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "vps": {
            "type": "string",
            "examples": [
              "smlouva/MSMT+-+MV"
            ],
            "pattern": "^smlouva/.+$"
          }
        }
      }
    }
  }
}