{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://data.gov.cz/otevřené-formální-normy/registr-práv-a-povinností/převod-činností-v-rámci-působnosti.schema.json",
  "type": "object",
  "required": [
    "položky"
  ],
  "properties": {
    "položky": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "činnost": {
            "type": "string",
            "examples": [
              "činnost/A1046/CR6074"
            ],
            "pattern": "^činnost/A[0-9]+/CR[0-9]+$"
          },
          "z-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A1046/12345678-1"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+-[0-9]+$"
          },
          "do-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A1046/98765432-1"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+-[0-9]+$"
          },
          "přebírající-ovm": {
            "type": "string",
            "examples": [
              "orgán-veřejné-moci/98765432-1"
            ],
            "pattern": "^orgán-veřejné-moci/[0-9]{8}-[0-9]+$"
          },
          "od": {
            "type": "string",
            "examples": [
              "2018-02-01"
            ],
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "do": {
            "type": "string",
            "examples": [
              "2018-02-28"
            ],
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "vps": {
            "type": "string",
            "examples": [
              "smlouva/ABCD1234"
            ],
            "pattern": "^smlouva/.+$"
          }
        }
      }
    }
  }
}