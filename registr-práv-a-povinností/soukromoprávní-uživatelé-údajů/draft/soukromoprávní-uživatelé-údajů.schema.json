{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://data.gov.cz/otevřené-formální-normy/registr-práv-a-povinností/soukromoprávní-uživatelé-údajů.json",
  "required": [
    "položky"
  ],
  "type": "object",
  "properties": {
    "položky": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "default": "soukromoprávní-uživatel-údajů",
            "pattern": "^soukromoprávní-uživatel-údajů$"
          },
          "id": {
            "type": "string",
            "examples": [
              "soukromoprávní-uživatel-údajů/12345678-1"
            ],
            "pattern": "^soukromoprávní-uživatel-údajů/[0-9]{8}-[0-9]+$"
          },
          "identifikátor": {
            "type": "string",
            "examples": [
              "12345678-1"
            ],
            "pattern": "^[0-9]{8}-[0-9]+$"
          },
          "název": {
            "type": "object",
            "properties": {
              "cs": {
                "type": "string",
                "examples": [
                  "Testovací soukromoprávní uživatel údajů 1"
                ]
              }
            }
          },
          "ičo": {
            "type": "string",
            "examples": [
              "12345678"
            ],
            "pattern": "^[0-9]{8}$"
          },
          "datové-schránky": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "default": "datová-schránka",
                  "pattern": "^datová-schránka$"
                },
                "id": {
                  "type": "string",
                  "examples": [
                    "datová-schránka/0abcdef"
                  ],
                  "pattern": "^datová-schránka/[0-9a-z]{7}$"
                },
                "typ-ds": {
                  "type": "string",
                  "examples": [
                    "typ-datové-schránky/FO"
                  ],
                  "pattern": "^typ-datové-schránky/[A-Z]+$"
                },
                "identifikátor-ds": {
                  "type": "string",
                  "examples": [
                    "0abcdef"
                  ],
                  "pattern": "^[0-9a-z]{7}$"
                }
              },
			  "examples": [
                {
				  "type": "datová-schránka",
				  "id": "datová-schránka/0abcdef",
	     		  "typ-ds": "typ-datové-schránky/FO",
				  "identifikátor-ds": "0abcdef"
			    }
			  ]
            }
          },
          "ustanovení": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "default": "ustanovení-právního-předpisu",
                  "pattern": "^ustanovení-právního-předpisu$"
                },
                "označení": {
                  "type": "string",
                  "examples": [
                    "§ 1 zákona č. 1/1000 Sb."
                  ]
                }
              }
            }
          },
          "seznam-kategorií": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "kategorie": {
                  "type": "string",
                  "examples": [
                    "kategorie-spuú/KS107"
                  ],
                  "pattern": "^kategorie-spuú/KS[0-9]+$"
                },
                "zařazení-od": {
                  "type": "string",
                  "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                },
                "zařazení-do": {
                  "type": "string",
                  "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
                }
              }
            }
          },
          "adresa-sídla": {
            "$ref": "#/definitions/adresní-místo"
          },
          "adresa-místa-podnikání": {
            "$ref": "#/definitions/adresní-místo"
          },
          "vznik": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "zánik": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "pozastavení-od": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "pozastavení-do": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "přerušení-od": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          },
          "přerušení-do": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
          }
        }
      }
    }
  },
  "definitions": {
    "adresní-místo": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "examples": [
            "https://linked.cuzk.cz/resource/ruian/adresni-misto/22305190"
          ],
          "pattern": "^https://linked.cuzk.cz/resource/ruian/adresni-misto/[0-9]{8}$"
        },
        "kód": {
          "type": "string",
          "examples": [
            "22305190"
          ],
          "pattern": "^[0-9]{8}$"
        }
      }
    }
  }
}