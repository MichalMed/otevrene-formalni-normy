{
  "@context": {
    "@base": "https://data.poskytovatel.cz/zdroj/",
    "položky": "@graph",
    "typ": "@type",
    "id": "@id",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "číselník": "skos:ConceptScheme",
    "položka": "skos:Concept",
    "název-číselníku": {
      "@id": "dct:title",
      "@container": "@language"
    },
    "položky": {
      "@reverse": "skos:inScheme",
      "@container": "@set"
    },
    "kód": {
      "@id": "skos:notation",
      "@type": "xsd:string"
    },
    "název": {
      "@id": "skos:prefLabel",
      "@container": "@language"
    }
  }
}