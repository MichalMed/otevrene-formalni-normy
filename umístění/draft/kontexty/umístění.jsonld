{
	"@context": {
		"@version": 1.1,
		"@import": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/věc/draft/kontexty/věc.jsonld",
		"@propagate": false,
		"umístění": "https://slovník.gov.cz/generický/umístění/pojem/",
		"v-sgov": "https://slovník.gov.cz/veřejný-sektor/pojem/",
		"locn": "http://www.w3.org/ns/locn#",
		"Umístění": {
			"@id": "dcterms:Location",
			"@context": {
				"název_umístění": "název",
				"popis_umístění": "popis",
				"geometrie": {
					"@id": "locn:geometry",
					"@context": "https://geojson.org/geojson-ld/geojson-context.jsonld"
				},
				"adresa": {
					"@id": "locn:address",
					"@context": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/adresy/draft/kontexty/adresa.jsonld"
				},
				"budova": {
					"@id": "locn:locatorName",
					"@container": "@language"
				},
				"patro": {
					"@id": "locn:locatorName",
					"@container": "@language"
				},
				"místnost": {
					"@id": "locn:locatorName",
					"@container": "@language"
				},
				"zahraniční_adresa_text": {
					"@id": "locn:fullAddress",
					"@container": "@language"
				}
			}
		}
	}
}