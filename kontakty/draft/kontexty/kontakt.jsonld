{
	"@context": {
		"@version": 1.1,
		"@import": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/věc/draft/kontexty/věc.jsonld",
		"@propagate": true,
		"kontakty": "https://slovník.gov.cz/generický/kontakty/pojem/",
		"vcard": "http://www.w3.org/2006/vcard/ns#",
		"Kontakt": {
			"@id": "vcard:VCard",
			"@context": {
				"email": {
					"@id": "vcard:hasEmail",
					"@type": "@id"
				},
				"facebook": {
					"@id": "kontakty:facebook",
					"@type": "xsd:string"
				},
				"idds": {
					"@id": "kontakty:má-datovou-schránku",
					"@type": "xsd:string"
				},
				"instagram": {
					"@id": "kontakty:instagram",
					"@type": "xsd:string"
				},
				"mobil": {
					"@id": "vcard:hasTelephone",
					"@type": "@id"
				},
				"telefon": {
					"@id": "vcard:hasTelephone",
					"@type": "@id"
				},
				"twitter": {
					"@id": "kontakty:twitter",
					"@type": "xsd:string"
				},
				"url": {
					"@id": "vcard:hasURL",
					"@type": "xsd:anyURI"
				},
				"umístění": {
					"@id": "kontakty:má-kontaktní-místo",
					"@context": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/umístění/draft/kontexty/umístění.jsonld"
				}
			}
		}
	}
}