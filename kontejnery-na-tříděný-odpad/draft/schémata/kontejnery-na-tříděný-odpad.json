{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://ofn.gov.cz/kontejnery-na-tříděný-odpad/draft/schémata/kontejnery-na-tříděný-odpad.json",
    "type": "array",
    "title": "Kontejnery na tříděný odpad",
    "items": {
        "$id": "#/items",
        "type": "object",
        "title": "Kontejnery na tříděný odpad",
        "properties": {
            "iri": {
                "$id": "#/items/properties/iri",
                "type": "string",
                "format": "iri",
                "title": "Identifikátor",
                "examples": [
                    "https://www.sako.cz/sberna-strediska-a-kontejnery/kontejner/Brno2458/"
                ]
            },
            "objem": {
                "$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/množství.json"
            },
            "nejbližší_vývoz": {
                "$id": "#/items/properties/datum",
                "type": "string",
                "format": "date",
                "title": "Nejbližší vývoz", 
                "examples": [
                    "2020-05-05"
                ]
            },
            "poznámka": {
                "$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/text.json"
            },
            "obrázek": {
                    "$id": "#/items/properties/obrázek",
                    "type": "string",
                    "format": "iri",
                    "title": "Obrázek",
                    "examples": [
                      "http://foto.turistika.cz/foto/30002/77410/full_f4efe4_znojemskehistorickevinobrani3.jpg"
                      ]
            },
            "pořadatel": {
                "type": "array",
                "items": {
                    "$ref": "https://ofn.gov.cz/lidé-a-osoby/draft/schémata/člověk-či-osoba.json"
                }
            },
            "hlavní_sponzor": {
                "type": "array",
                "items": {
                    "$ref": "https://ofn.gov.cz/lidé-a-osoby/draft/schémata/člověk-či-osoba.json"
                }
            },
            "sponzor": {
                "type": "array",
                "items": {
                    "$ref": "https://ofn.gov.cz/lidé-a-osoby/draft/schémata/člověk-či-osoba.json"
                }
            },
            "mediální_partner": {
                "type": "array",
                "items": {
                    "$ref": "https://ofn.gov.cz/lidé-a-osoby/draft/schémata/člověk-či-osoba.json"
                }
            },
            "umístění": {
                "type": "array",
                "items": {
                    "$ref": "https://ofn.gov.cz/umístění/draft/schémata/umístění.json"
                }
            },
            "trvání": {
                "type": "array",
                "items": {
                "$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-interval.json"
                }
            },
            "kontakt": {
                "type": "array",
                "items": {
                    "$ref": "https://ofn.gov.cz/kontakty/draft/schémata/kontakt.json"
                }
            },
            "vstupné": {
                "type": "array",
                "items": {
                    "$ref": "https://ofn.gov.cz/vstupné/draft/schémata/vstupné.json"
                }
            }
        }
    }
}