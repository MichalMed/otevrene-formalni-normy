{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "https://ofn.gov.cz/přílohy/draft/schémata/příloha.json",
	"type": "object",
	"title": "Příloha",
	"required": ["typ"],
	"properties": {
		"typ": {
			"const": "Příloha"
		},
		"url": {
			"id": "#/properties/url",
			"type": "string",
			"format": "url",
			"title": "URL",
			"examples": [
				"https://www.brno.cz/fileadmin/user_upload/sprava_mesta/magistrat_mesta_brna/PO/volna_mista/VR_067_20_OUPR_urbanista_-_bydleni_05.docx"
			]
		},
		"autor": {
			"type": "array",
			"items": {
				"$ref": "https://ofn.gov.cz/lidé-a-osoby/draft/schémata/člověk-či-osoba.json"
			}
		},
		"typ_media": {
			"id": "#/properties/typ_media",
			"type": "string",
			"format": "iri",
			"pattern": "^http\\:\/\/www\\.iana\\.org\/assignments\/media-types\/.*$",
			"title": "Typ souboru",
			"examples": [
				"http://www.iana.org/assignments/media-types/image/tiff"
			]
		},
		"podmínky_užití": {
			"$ref": "https://ofn.gov.cz/podmínky-užití/draft/schémata/podmínky-užití.json"
		}
	}
}