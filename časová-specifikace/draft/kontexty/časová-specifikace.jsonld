{
	"@context": {
		"@version": 1.1,
		"@import": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/věc/draft/kontexty/věc.jsonld",
		"@propagate": true,
		"čas": "https://slovník.gov.cz/generický/čas/pojem/",
		"doba": "https://slovník.gov.cz/generický/časová-doba/pojem/",
		"frekvence": "https://slovník.gov.cz/generický/specifická-frekvence/pojem/",
		"Časová specifikace": {
			"@id": "https://slovník.gov.cz/základní/pojem/časová-specifikace",
			"@context": {
				"počet_opakování": {
					"@id": "čas:počet-opakování",
					"@type": "xsd:integer"
				},
				"časový_interval": {
					"@id": "čas:časový-interval",
					"@context": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/základní-datové-typy/draft/kontexty/časový-interval.jsonld"
				},
				"jiná_časová_specifikace": {
					"@id": "čas:jiná-časová-specifikace",
					"@type": "@id"
				},
				"den_v_týdnu": {
					"@id": "čas:den-v-týdnu",
					"@type": "@id"
				},
				"časové_období": {
					"@id": "čas:časové-období",
					"@type": "@id"
				},
				"frekvence": {
					"@id": "čas:frekvence",
					"@type": "@id"
				},
				"časová_doba": {
					"@id": "čas:má-časovou-dobu",
					"@context": {
						"Časová doba": {
							"@id": "doba:časová-doba",
							"@context": {
								"čas": {
									"@id": "doba:čas",
									"@type": "xsd:time"
								},
								"od": {
									"@id": "doba:od",
									"@type": "xsd:time"
								},
								"do": {
									"@id": "doba:do",
									"@type": "xsd:time"
								}
							}
						}
					}
				},
				"specifická_frekvence": {
					"@id": "čas:má-specifickou-frekvenci",
					"@context": {
						"Specifická frekvence": {
							"@id": "frekvence:specifická-frekvence",
							"@context": {
								"minuta": {
									"@id": "frekvence:minuta",
									"@type": "xsd:integer"
								},
								"hodina": {
									"@id": "frekvence:hodina",
									"@type": "xsd:integer"
								},
								"den_v_měsíci": {
									"@id": "frekvence:den-v-měsíci",
									"@type": "xsd:integer"
								},
								"týden_v_měsíci": {
									"@id": "frekvence:týden-v-měsíci",
									"@type": "xsd:integer"
								},
								"týden_v_roku": {
									"@id": "frekvence:týden-v-roku",
									"@type": "xsd:integer"
								},
								"rok_v_desetiletí": {
									"@id": "frekvence:rok-v-desetiletí",
									"@type": "xsd:integer"
								},
								"rok_ve_století": {
									"@id": "frekvence:rok-ve-století",
									"@type": "xsd:integer"
								}
							}
						}
					}
				},
				"výjimka": {
					"@id": "čas:výjimka",
					"@context": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/časová-specifikace/draft/kontexty/časová-specifikace.jsonld"
				},
				"časová_platnost": {
					"@id": "čas:výjimka",
					"@context": "https://pod-test.mvcr.gov.cz/otevřené-formální-normy/časová-specifikace/draft/kontexty/časová-specifikace.jsonld"
				}
			}
		}
	}
}