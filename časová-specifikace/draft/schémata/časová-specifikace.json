{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://ofn.gov.cz/časová-specifikace/draft/schémata/časová-specifikace.json",
	"type": "object",
	"title": "Časová specifikace",
	"properties": {
		"iri": {
			"$id": "#/properties/iri",
			"type": "string",
			"format": "iri",
			"title": "Identifikátor",
			"examples": [
				"https://www.trebic.cz/zdroj/aktualita/2020/dopadení-lupiče-na-francouzské-ulici"
			]
		},
		"název": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/text.json"
		},
		"popis": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/text.json"
		},
		"vytvořeno": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-okamžik.json"
		},
		"relevantní_do": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-okamžik.json"
		},
		"zneplatněno": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-okamžik.json"
		},
		"příloha": {
			"$ref": "https://ofn.gov.cz/přílohy/draft/schémata/přílohy.json"
		},
		"počet_opakování": {
			"$id": "#/properties/počet-opakování",
			"type": "integer",
			"title": "Počet opakování",
			"examples": [
				3
			]
		},
		"časový_interval": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-interval.json"
		},
		"jiná_časová_specifikace": {
			"$id": "#/properties/jiná_časová_specifikace",
			"type": "array",
			"items": {
				"type": "string",
				"format": "iri",
				"pattern": "^https\\:\/\/data\\.mvcr\\.gov\\.cz\/zdroj\/číselníky\/jiná-časová-specifikace\/položky\/.*$",
				"title": "Jiná časová specifikace",
				"examples": [
					"https://data.mvcr.gov.cz/zdroj/číselníky/jiná-časová-specifikace/položky/dobré-počasí"
				]
			}
		},
		"den_v_týdnu": {
			"$id": "#/properties/den-v-týdnu",
			"type": "array",
			"items": {
				"type": "string",
				"format": "iri",
				"pattern": "^https\\:\/\/data\\.mvcr\\.gov\\.cz\/zdroj\/číselníky\/dny-v-týdnu\/položky\/.*$",
				"title": "Den v týdnu",
				"examples": [
					"https://data.mvcr.gov.cz/zdroj/číselníky/dny-v-týdnu/položky/pondělí"
				]
			}
		},
		"časová_doba": {
			"id": "#/properties/časová-doba",
			"type": "array",
			"items": {
				"type": "string",
				"format": "iri",
				"pattern": "^http\\:\/\/publications\\.europa\\.eu\/resource\/authority\/timeperiod\/.*$",
				"title": "Časová doba",
				"examples": [
					"http://publications.europa.eu/resource/authority/timeperiod/OCT"
				]
			}
		},
		"frekvence": {
			"id": "#/properties/frekvence",
			"type": "array",
			"items": {
				"type": "string",
				"format": "iri",
				"pattern": "^http\\:\/\/publications\\.europa\\.eu\/resource\/authority\/frequency\/.*$",
				"title": "Frekvence",
				"examples": [
					"http://publications.europa.eu/resource/authority/frequency/BIWEEKLY"
				]
			}
		},
		"specifická_frekvence": {
			"$id": "#/properties/specifická-frekvence",
			"type": "array",
			"title": "Specifická frekvence",
			"items": {
				"$id": "#/properties/specifická_frekvence/items/specifická-frekvence",
				"type": "object",
				"title": "Specifická frekvence",
				"properties": {
					"minuta": {
						"$id": "#/properties/specifická_frekvence/properties/minuta",
						"type": "integer",
						"title": "Minuta",
						"examples": [
							20
						]
					},
					"hodina": {
						"$id": "#/properties/specifická_frekvence/properties/hodina",
						"type": "integer",
						"title": "Hodina",
						"examples": [
							20
						]
					},
					"den_v_měsíci": {
						"$id": "#/properties/specifická_frekvence/properties/den_v_měsicí",
						"type": "integer",
						"title": "Den v měsíci",
						"examples": [
							20
						]
					},
					"týden_v_měsíci": {
						"$id": "#/properties/specifická_frekvence/properties/týden_v_měsíci",
						"type": "integer",
						"title": "Týden v měsíci",
						"examples": [
							2
						]
					},
					"týden_v_roku": {
						"$id": "#/properties/specifická_frekvence/properties/týden_v_roku",
						"type": "integer",
						"title": "Týden v roku",
						"examples": [
							20
						]
					},
					"rok_v_desetiletí": {
						"$id": "#/properties/specifická_frekvence/properties/rok_v_desetiletí",
						"type": "integer",
						"title": "Týden v měsíci",
						"examples": [
							2
						]

					},
					"rok_ve_století": {
						"$id": "#/properties/specifická_frekvence/properties/rok_ve_století",
						"type": "integer",
						"title": "Rok ve století",
						"examples": [
							20
						]
					}
				}
			}
		},
		"výjimka": {
			"$ref": "https://ofn.gov.cz/časová-specifikace/draft/schémata/seznam-časových-specifikací.json"
		},
		"časová_platnost": {
			"$ref": "https://ofn.gov.cz/časová-specifikace/draft/schémata/seznam-časových-specifikací.json"
		}
	}
}