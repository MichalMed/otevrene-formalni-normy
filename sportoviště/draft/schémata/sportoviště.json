{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://ofn.gov.cz/sportoviště/draft/schémata/sportoviště.json",
    "type": "array",
    "title": "Sportoviště",
    "items": {
        "$id": "#/items",
